{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "baseUrl": "https://renems.com/",
  "version": "0.1.0",
  "author": "RMS <contact@renems.com>",
  "scripts": {
    "start": "npm run develop",
    "dev": "gatsby develop",
    "develop": "gatsby develop",
    "clean": "gatsby clean",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "setup": "npm install && npm run validate",
    "validate": "npm run build",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "prettify": "pretty-quick",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|css|less|scss|graphql|md|mdx)\"",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "analyze": "cross-env-shell ANALYZE_BUNDLE_SIZE=true & npm run build",
    "deploy": "npm run deploy:netlify",
    "deploy:netlify": "netlify deploy --dir=./public -p -m \"$(git log -1 --pretty=%B)\""
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.7",
    "@mdx-js/react": "^1.5.7",
    "aos": "^3.0.0-beta.6",
    "cross-env": "7.0.2",
    "gatsby": "^2.19.5",
    "gatsby-cli": "^2.8.27",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-manifest": "^2.2.38",
    "gatsby-plugin-mdx": "^1.0.75",
    "gatsby-plugin-netlify": "^2.1.31",
    "gatsby-plugin-offline": "^3.0.32",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sass": "^2.1.27",
    "gatsby-plugin-sharp": "^2.4.0",
    "gatsby-plugin-styled-components": "^3.1.18",
    "gatsby-plugin-transition-link": "^1.17.7",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-remark": "^2.6.48",
    "gatsby-transformer-sharp": "^2.3.13",
    "gsap": "^3.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^16.8.6",
    "react-helmet": "^6.0.0",
    "react-icons": "^3.8.0",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-eslint": "10.x",
    "eslint": "6.x",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "graphql-tools": "^5.0.0",
    "husky": "^4.2.5",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^10.1.3",
    "node-sass": "^4.13.1",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js|jsx|ts|tsx|json|yml|yaml|css|less|scss|graphql|md|mdx}": [
      "pretty-quick --staged",
      "git add"
    ]
  },
  "keywords": [
    "portfolio",
    "gatsby",
    "react"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/remusa/renems.com/issues"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ]
}
